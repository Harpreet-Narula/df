a = int(input("enter the number 1:"))
print a
b = int(input("enter the number 2:"))
print b
c = int(input("enter the number 3:"))
print c


if (a*a == b*b + c*c):
    print ("a*a == b*b + c*c")
if (b*b == a*a + c*c) :
    print("b*b == a*a + c*c")
if (c*c == b*b  + a*a):
    print ("c*c == b*b  + a*a")
if (a*a != b*b + c*c) and (b*b != a*a + c*c) and (c*c != b*b  + a*a):
    print("Not possible")
 
 
 
 
 
 
 #1 - Read three integers from user and print out the maximum of the three
user_number1 = int(input("enter the number 1:"))
print user_number1
user_number2 = int(input("enter the number 2:"))
print user_number2
user_number3 = int(input("enter the number 3:"))
print user_number3

if user_number1 > user_number2:
    max=user_number1
if user_number2 > user_number3:
    max= user_number2
if user_number3 > user_number1:
    max= user_number3
print ("The greatest number is " + str(max))


